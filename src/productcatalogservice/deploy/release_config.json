{
  "release": {
  "name": "product-catalogue",
  "strategy": {
    "type": "canary",
    "config":{
      "interval": "30s",
      "initial_traffic": 10,
      "traffic_step": 10,
      "max_traffic": 100
    }
  },
  "monitor": {
    "checks": [
      {
        "name": "request-success",
        "min": 99
      },
      {
        "name": "request-duration",
        "min": 0,
        "max": 1500
      }
    ]
  }
}}